    "from pathlib import Path\n",
    "import json, os\n",
    "from torchvision import datasets\n",
    "from PIL import Image\n",
    "\n",
    "(IMAGES_DIR).mkdir(parents=True, exist_ok=True)\n",
    "categories = [{\"id\":i, \"name\":str(i)} for i in range(43)]\n",
    "\n",
    "if not Path(COCO_ANN).exists():\n",
    "    # raw (no-transform) test set to pull a few images from\n",
    "    gtsrb_test_raw = datasets.GTSRB(root=str(DATA_ROOT), split=\"test\", download=True, transform=None)\n",
    "\n",
    "    images_json, ann_json = [], []\n",
    "    ann_id = 1\n",
    "    num_samples = min(300, len(gtsrb_test_raw))  # small demo set\n",
    "\n",
    "    for idx in range(num_samples):\n",
    "        img_pil, label = gtsrb_test_raw[idx]   # label in [0..42]\n",
    "        w, h = img_pil.size\n",
    "\n",
    "        # save image\n",
    "        fname = f\"{idx:06d}.png\"\n",
    "        img_pil.save(IMAGES_DIR / fname)\n",
    "\n",
    "        # image record\n",
    "        images_json.append({\"id\": idx+1, \"file_name\": fname, \"width\": w, \"height\": h})\n",
    "\n",
    "        # whole-image bbox as a simple demo\n",
    "        bbox = [0, 0, w, h]\n",
    "        ann_json.append({\n",
    "            \"id\": ann_id,\n",
    "            \"image_id\": idx+1,\n",
    "            \"category_id\": int(label),\n",
    "            \"bbox\": bbox,\n",
    "            \"area\": float(w*h),\n",
    "            \"iscrowd\": 0\n",
    "        })\n",
    "        ann_id += 1\n",
    "\n",
    "    coco = {\"images\": images_json, \"annotations\": ann_json, \"categories\": categories}\n",
    "    with open(COCO_ANN, \"w\") as f:\n",
    "        json.dump(coco, f)\n",
    "    print(\"Created toy COCO file at:\", COCO_ANN)\n",
    "else:\n",
    "    print(\"COCO file already exists:\", COCO_ANN)\n"
   ]

    "class CocoDetection(Dataset):\n",
    "    def __init__(self, img_dir, ann_file, processor, transforms=None):\n",
    "        self.img_dir = Path(img_dir)\n",
    "        self.processor = processor\n",
    "        self.transforms = transforms\n",
    "\n",
    "        with open(ann_file, \"r\") as f:\n",
    "            ann = json.load(f)\n",
    "\n",
    "        self.images = ann[\"images\"]                  # list of {id, file_name, width, height}\n",
    "        self.annotations = ann[\"annotations\"]        # list of {image_id, category_id, bbox, ...}\n",
    "        self.categories = ann.get(\"categories\", [])  # optional\n",
    "\n",
    "        # index annotations by image_id\n",
    "        self.by_img = {}\n",
    "        for a in self.annotations:\n",
    "            self.by_img.setdefault(a[\"image_id\"], []).append(a)\n",
    "\n",
    "    def __len__(self):\n",
    "        return len(self.images)\n",
    "\n",
    "    def __getitem__(self, idx):\n",
    "        info = self.images[idx]\n",
    "        img_path = self.img_dir / info[\"file_name\"]\n",
    "        image = Image.open(img_path).convert(\"RGB\")\n",
    "\n",
    "        anns = self.by_img.get(info[\"id\"], [])\n",
    "        # Ensure required keys are present\n",
    "        norm_anns = []\n",
    "        for a in anns:\n",
    "            x, y, w, h = a[\"bbox\"]\n",
    "            area = a.get(\"area\", float(w * h))\n",
    "            iscrowd = a.get(\"iscrowd\", 0)\n",
    "            norm_anns.append({\n",
    "                \"bbox\": [float(x), float(y), float(w), float(h)],  # COCO xywh\n",
    "                \"category_id\": int(a[\"category_id\"]),\n",
    "                \"area\": float(area),\n",
    "                \"iscrowd\": int(iscrowd),\n",
